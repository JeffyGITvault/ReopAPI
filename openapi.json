{
  "openapi": "3.1.0",
  "info": {
    "title": "Get SEC Filings Data",
    "description": "Gets recent 10-Q filings for a public company, including filing dates and validated HTML links. Defaults to 2 filings unless 'count' is set (1‚Äì4 allowed). Link validation uses HEAD requests with GET fallback for reliability.",
    "version": "v4.3.3"
  },
  "servers": [
    {
      "url": "https://secapi-5s5x.onrender.com"
    }
  ],
  "paths": {
    "/get_quarterlies/{company_name}": {
      "get": {
        "description": "Gets recent 10-Q filings for a public company, including filing dates and validated HTML links. Defaults to 2 filings unless 'count' is set (1‚Äì4 allowed). Link validation uses HEAD requests with GET fallback for reliability.",
        "operationId": "GetQuarterlyFilings",
        "parameters": [
          {
            "name": "company_name",
            "in": "path",
            "description": "Public company name or ticker (e.g., Apple, AAPL, Meta, RH).",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "count",
            "in": "query",
            "description": "Optional. Number of most recent 10-Q filings to return (default is 2, max is 4).",
            "required": false,
            "schema": {
              "type": "integer",
              "default": 2,
              "minimum": 1,
              "maximum": 4
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Quarterly filings data",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "Matched Company Name": {
                      "type": "string",
                      "description": "Resolved company name using alias and fuzzy matching."
                    },
                    "CIK": {
                      "type": "string",
                      "description": "SEC Central Index Key for the matched company."
                    },
                    "10-Q Filings": {
                      "type": "array",
                      "description": "A list of the latest 10-Q reports with filing dates and HTML URLs.",
                      "items": {
                        "type": "object",
                        "properties": {
                          "Filing Date": {
                            "type": "string",
                            "description": "The official SEC filing date (YYYY-MM-DD)."
                          },
                          "HTML Report": {
                            "type": "string",
                            "description": "Validated URL to the HTML version of the 10-Q report."
                          },
                          "Status": {
                            "type": "string",
                            "description": "Validation status (e.g., Validated, Unavailable)."
                          },
                          "DisplayIndex": {
                            "type": "string",
                            "description": "Optional visual index for display (e.g., 1Ô∏è‚É£, 2Ô∏è‚É£)."
                          },
                          "Marker": {
                            "type": "string",
                            "description": "Optional label (e.g., üìå Most Recent, üïì Older)."
                          }
                        },
                        "required": [
                          "Filing Date",
                          "HTML Report",
                          "Status"
                        ]
                      }
                    }
                  },
                  "required": [
                    "Matched Company Name",
                    "CIK",
                    "10-Q Filings"
                  ]
                }
              }
            }
          },
          "404": {
            "description": "Company not found or no filings available"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {}
  }
}
