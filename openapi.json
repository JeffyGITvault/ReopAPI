{
  "openapi": "3.1.0",
  "info": {
    "title": "Get SEC Filings Data",
    "description": "Fetches latest 10-Q and 10-K filings. Supports alias learning, GitHub persistence, and clean CIK resolution.",
    "version": "v4.2.5"
  },
  "servers": [
    {
      "url": "https://secapi-5s5x.onrender.com"
    }
  ],
  "paths": {
    "/get_filings/{company_name}": {
      "get": {
        "description": "Returns the latest 10-Q HTML report and 10-K HTML or Excel link for a public company or ticker.",
        "operationId": "GetSECFilings",
        "parameters": [
          {
            "name": "company_name",
            "in": "path",
            "description": "Public company name or ticker (e.g. Apple, AAPL, Meta, RH).",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Filing links and matched metadata",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "Matched Company Name": {
                      "type": "string",
                      "description": "Matched company name from SEC."
                    },
                    "CIK": {
                      "type": "string",
                      "description": "Central Index Key used to pull filings."
                    },
                    "10-Q Filing": {
                      "type": ["string", "null"],
                      "description": "Direct URL to latest 10-Q HTML report (if any)."
                    },
                    "10-K Filing": {
                      "type": ["string", "null"],
                      "description": "Direct URL to latest 10-K HTML report, or Excel financials if HTML is not available."
                    }
                  },
                  "required": [
                    "Matched Company Name",
                    "CIK",
                    "10-Q Filing",
                    "10-K Filing"
                  ]
                }
              }
            }
          },
          "404": {
            "description": "No match or filings found."
          }
        }
      }
    }
  },
  "components": {
    "schemas": {}
  }
}
