{
  "openapi": "3.1.0",
  "info": {
    "title": "Get SEC Filings Data",
    "description": "Fetches the latest 10-Q and 10-K filings. Prioritizes HTML filings and falls back to Excel reports if available. Includes alias learning, GitHub persistence, and CIK resolution.",
    "version": "v4.2.6"
  },
  "servers": [
    {
      "url": "https://secapi-5s5x.onrender.com"
    }
  ],
  "paths": {
    "/get_filings/{company_name}": {
      "get": {
        "description": "Returns the most recent 10-Q and 10-K filings for a public company, based on name or ticker symbol.",
        "operationId": "GetSECFilings",
        "parameters": [
          {
            "name": "company_name",
            "in": "path",
            "description": "Public company name or ticker (e.g., Apple, AAPL, Meta, RH).",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Filing links and matched metadata",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "Matched Company Name": {
                      "type": "string",
                      "description": "Matched company name from SEC."
                    },
                    "CIK": {
                      "type": "string",
                      "description": "Central Index Key used to pull filings."
                    },
                    "10-Q Filing": {
                      "type": ["string", "null"],
                      "description": "URL to most recent 10-Q HTML report (or message if not found)."
                    },
                    "10-K Filing": {
                      "type": ["string", "null"],
                      "description": "URL to most recent 10-K HTML or Excel report (prefers HTML)."
                    }
                  },
                  "required": [
                    "Matched Company Name",
                    "CIK",
                    "10-Q Filing",
                    "10-K Filing"
                  ]
                }
              }
            }
          },
          "404": {
            "description": "No match or filings found."
          }
        }
      }
    }
  },
  "components": {
    "schemas": {}
  }
}
